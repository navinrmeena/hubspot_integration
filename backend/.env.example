# Environment Variables for HubSpot Integration Backend
# Copy this file to .env and fill in your actual API keys

# Airtable API Credentials
# Get these from: https://airtable.com/create/tokens
AIRTABLE_CLIENT_ID=your_airtable_client_id_here
AIRTABLE_CLIENT_SECRET=your_airtable_client_secret_here

# Notion API Credentials
# Get these from: https://www.notion.so/my-integrations
NOTION_CLIENT_ID=your_notion_client_id_here
NOTION_CLIENT_SECRET=your_notion_client_secret_here

# HubSpot API Credentials
# Get these from: https://developers.hubspot.com/
HUBSPOT_CLIENT_ID=your_hubspot_client_id_here
HUBSPOT_CLIENT_SECRET=your_hubspot_client_secret_here

# Redis Configuration
# Default Redis settings - adjust if using different Redis instance
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Server Configuration
# Backend server settings
HOST=127.0.0.1
PORT=8000

# Optional: Database Configuration (if you add database support later)
# DATABASE_URL=postgresql://user:password@localhost:5432/dbname
# MONGODB_URL=mongodb://localhost:27017/dbname

# Optional: Logging Configuration
# LOG_LEVEL=INFO
# LOG_FILE=logs/app.log
